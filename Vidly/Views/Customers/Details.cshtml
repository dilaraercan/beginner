@model Vidly.Models.Customer
@{
    ViewBag.Title = "Model.Name";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2> @*we returned the model names not an ordinary text*@

<ul>
    <li>@Model.MembershipType.MembershipName</li>
    @if (Model.bDay != null)
    {
        <li>@Model.bDay</li>
    }
</ul>  